# OnchainKit API Key (required for Base integration)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_onchainkit_api_key_here

# OpenAI API Key (for AI-generated legal content)
OPENAI_API_KEY=your_openai_api_key_here

# Alternative: OpenRouter API Key (for AI content generation)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Supabase Configuration (for data storage)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Privy Configuration (for user authentication)
NEXT_PUBLIC_PRIVY_APP_ID=your_privy_app_id_here

# Stripe Configuration (for payments)
STRIPE_SECRET_KEY=your_stripe_secret_key_here
